<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
    <profiles>
        <profile>
            <id>cdc</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>imagehub</id>
                    <url>https://imagehub.cdc.gov/repository/maven-dpei/</url>
                </repository>

            </repositories>

            <pluginRepositories>
                <pluginRepository>
                    <id>imagehub</id>
                    <url>https://imagehub.cdc.gov/repository/maven-dpei/</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>cdc</activeProfile>
    </activeProfiles>
</settings>